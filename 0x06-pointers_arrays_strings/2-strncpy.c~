#include "main.h"
#include <stdio.h>

/**
 * char *_strncpy(char *dest, char *src, int n)  copies a string.
 *
 * Return: Always 0.
 */
char *_strncpy(char *dest, char *src, int n)
{
  n = 0;
  while(dest[n] != '\0')
    {
      src[n] = dest[n];
      n++;
    }
  src[n] != '\0';
  putchar(dest);
  return(0);
}
